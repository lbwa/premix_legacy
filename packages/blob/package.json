{
  "name": "@premix/blob",
  "version": "0.2.0",
  "description": "blob converter",
  "keywords": [
    "blob",
    "base64"
  ],
  "author": "Bowen <mr_lbw@outlook.com>",
  "homepage": "https://github.com/lbwa/premix/tree/master/packages/blob#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbwa/premix.git"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf lib/*",
    "test": "jest --coverage"
  },
  "bugs": {
    "url": "https://github.com/lbwa/premix/issues"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "\\\\node_modules\\\\"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "preset": "ts-jest",
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\\\node_modules\\\\",
      "\\\\lib\\\\"
    ]
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  }
}